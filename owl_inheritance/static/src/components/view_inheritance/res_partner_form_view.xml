<?xml version="1.0" encoding="UTF-8" ?>
<templates>
    <t t-name="owl_inheritance.ResPartnerFormView" t-inherit="web.FormView" owl="1">
        <xpath expr="//button[hasclass('o_form_button_create')]" position="after">
            <button class="btn btn-primary" t-on-click="()=>this.openWebsite(model.root.data.website)">Go to Website</button>
        </xpath>
        <xpath expr="//button[contains(@class, 'btn btn-secondary o_form_button_create')]" position="after">
            <button class="btn btn-primary" t-on-click="()=>this.openWebsite(model.root.data.website)">Go to Website</button>
        </xpath>
        <!-- <xpath expr="//button[hasclass('btn btn-secondary o_form_button_create')]" position="after">
            <button class="btn btn-primary" t-on-click="()=>this.openWebsite(model.root.data.website)">Go to Website</button>
        </xpath> -->
        <!-- <xpath expr="//button[hasclass('btn btn-outline-primary o_form_button_create')]" position="after">
            <button class="btn btn-primary" t-on-click="()=>this.openWebsite(model.root.data.website)">Go to Website</button>
        </xpath> -->
        <xpath expr="//button[@data-hotkey='c']" position="after">
            <button class="btn btn-primary" t-on-click="()=>this.openWebsite(model.root.data.website)">Go to Website</button>
        </xpath>
        <xpath expr="//t[contains(@t-if, 'canCreate')]" position="after">
            <button class="btn btn-primary" t-on-click="()=>this.openWebsite(model.root.data.website)">Go to Website</button>
        </xpath>
        <xpath expr="//button[@class='btn btn-secondary o_form_button_create']" position="after">
            <button class="btn btn-primary" t-on-click="()=>this.openWebsite(model.root.data.website)">Go to Website</button>
        </xpath>
    </t>
</templates>